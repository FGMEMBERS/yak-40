<PropertyList>
 <path>ark.ac</path>


<!--Левый полукомлект--> 
<!--Десятки-->
<animation>
  <type>rotate</type>
  <object-name>left</object-name>
  <object-name>tens_l</object-name>
  <property>yak-40/instrumentation/ark_l/tens_l</property>
  <interpolation>
    <entry><ind>  0.0</ind><dep>-36.0</dep></entry>
   <entry><ind>  9.0</ind><dep>288.0</dep></entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>-0.01870</y-m>
   <z-m>-0.02596</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
<!--Крутить мышиным колесом--> 
 <animation>
  <type>pick</type>
  <object-name>left</object-name>
  <object-name>tens_l</object-name>
  <visible>true</visible>
  <action>
    <button>3</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>yak-40/instrumentation/ark_l/tens_l</property>
      <step>1</step>
      <min>0.0</min>
      <max>9.0</max>
    </binding>
  </action>
</animation>
<animation>
  <type>pick</type>
  <object-name>left</object-name>
  <object-name>tens_l</object-name>
  <visible>true</visible>
  <action>
    <button>4</button>
    <repeatable>false</repeatable>
