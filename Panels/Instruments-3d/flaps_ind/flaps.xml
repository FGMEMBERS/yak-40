<PropertyList>
 <path>flaps.ac</path>


 
<animation>
  <type>rotate</type>
  <object-name>needle</object-name>
  <property>fdm/jsbsim/fcs/flap-pos-deg</property>
  <interpolation>
    <entry><ind>  0.0</ind><dep>0.0</dep></entry>
   <entry><ind>  30.0</ind><dep>45.0</dep></entry> 
   <entry><ind> 60</ind><dep>  89</dep></entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>-0.008</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 
               <!--Lights-->
                                   
 <animation>
   <type>material</type>
   <object-name>emi</object-name>
   <object-name>rect1</object-name>
   <object-name>rect2</object-name>
   <object-name>rect3</object-name>
   <emission>
	<red-prop>/IL-76/instrumentation/lights/centr_panel/int-red</red-prop>
    <green-prop>/IL-76/instrumentation/lights/centr_panel/int-green</green-prop>
    <blue-prop>/IL-76/instrumentation/lights/centr_panel/int-blue</blue-prop>
	</emission>
</animation>


 </PropertyList>

