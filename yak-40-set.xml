<input>
  <keyboard>

<!-- Nominal -->
    <key n="78">
      <name>N</name>
      <desc>Nominal thrust 95</desc>
      <binding>
	<command>property-assign</command>
	<property>/controls/engines/engine[0]/throttle</property>
	<value>0.880</value>
      </binding>
      <binding>
	<command>property-assign</command>
	<property>/controls/engines/engine[1]/throttle</property>
	<value>0.880</value>
      </binding>
      <binding>
	<command>property-assign</command>
	<property>/controls/engines/engine[2]/throttle</property>
	<value>0.880</value>
      </binding>
    </key>

    <key n="110">
      <name>n</name>
      <desc>90</desc>
      <binding>
	<command>property-assign</command>
	<property>/controls/engines/engine[0]/throttle</property>
	<value>0.778</value>
      </binding>
      <binding>
	<command>property-assign</command>
	<property>/controls/engines/engine[1]/throttle</property>
	<value>0.778</value>
      </binding>
      <binding>
	<command>property-assign</command>
	<property>/controls/engines/engine[2]/throttle</property>
	<value>0.778</value>
      </binding>
    </key>

    <key n="14">
      <name>Ctrl+N</name>
      <desc>78</desc>
      <binding>
	<command>property-assign</command>
	<property>/controls/engines/engine[0]/throttle</property>
	<value>0.5248</value>
      </binding>
      <binding>
	<command>property-assign</command>
	<property>/controls/engines/engine[1]/throttle</property>
	<value>0.5248</value>
      </binding>
      <binding>
	<command>property-assign</command>
	<property>/controls/engines/engine[2]/throttle</property>
	<value>0.5248</value>
      </binding>
    </key>
    
<!--View shortcuts-->
    <key n="96">
      <name>~</name>
      <desc>Custom view modifier</desc>
      <binding>
        <command>nasal</command>
        <script>view.modView(1)</script>
      </binding>
      <mod-up>
        <binding>
          <command>nasal</command>
          <script>view.modView(0)</script>
        </binding>
      </mod-up>
    </key>
    
       <key n="49">
      <name>1</name>
      <desc>Captain view</desc>
      <binding>
